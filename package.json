  "dependencies": {
    "bcrypt": "^5.1.0",
    "big-integer": "^1.6.51",
    "circomlibjs": "^0.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ether": "^0.0.9",
    "ethers": "^6.6.3",
    "express": "^4.18.2",
